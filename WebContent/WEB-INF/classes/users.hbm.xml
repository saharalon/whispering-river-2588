<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="il.ac.shenkar.JavaTunes.User" table="USERS">
		<id name="username">
			<column name="USERNAME"/>
		</id>
		<property name="password">
			<column name="PASSWORD"/>
		</property>
		<property name="cart">
			<column name="CART"/>
		</property>
		<property name="purchasedTracks">
			<column name="PURCHASED_TRACKS"/>
		</property>
		<property name="playlist">
			<column name="PLAYLIST"/>
		</property>
	</class>
</hibernate-mapping>